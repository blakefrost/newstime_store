
<div class="container lead-space">

  <h1>Users</h1>

  <table>
    <tr>
      <th>Username</td>
      <th>Email</td>
      <th></td>
    </tr>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.username %></td>
        <td><%= user.email %></td>
        <td><%= link_to "View", admin_user_url(user) %></td>
      </tr>
    <% end %>
  </table>




</div>
